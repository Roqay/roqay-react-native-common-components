{
  "name": "roqay-react-native-common-components-example",
  "description": "Example app for roqay-react-native-common-components",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "pods": "run-script-os",
    "pods:macos": "npx pod-install",
    "pods:default": "echo '\"pods\" command available on mac only!'",
    "android": "react-native run-android",
    "ios-clean": "run-script-os",
    "ios-clean:macos": "cd ios && rm -rf build && xcodebuild clean && cd ..",
    "ios-clean:default": "echo '\"ios-clean\" command available on mac only!'",
    "ios-simulator": "run-script-os",
    "ios-simulator:macos": "react-native run-ios --simulator=\"iPhone 14 Pro Max\"",
    "ios-simulator:default": "echo '\"ios-simulator\" command available on mac only!'",
    "ios": "run-script-os",
    "ios:macos": "react-native run-ios",
    "ios:default": "echo '\"ios\" command available on mac only!'",
    "start": "react-native start",
    "postinstall": "patch-package && yarn pods && yarn android-clean && yarn ios-clean"
  },
  "dependencies": {
    "@klarna/react-native-vector-drawable": "^0.4.0",
    "@react-native-firebase/app": "^18.1.0",
    "@react-native-firebase/crashlytics": "^18.1.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "react": "18.2.0",
    "react-native": "0.71.12",
    "react-native-fast-image": "^8.6.3",
    "react-native-paper": "^5.9.1",
    "react-native-progress": "^5.0.0",
    "react-native-safe-area-context": "^4.6.4",
    "react-native-screens": "^3.22.1",
    "react-native-size-matters": "^0.4.0",
    "react-native-svg": "^13.10.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-vector-image": "^0.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/runtime": "^7.22.6",
    "babel-plugin-module-resolver": "^5.0.0",
    "metro-react-native-babel-preset": "0.73.10",
    "patch-package": "^7.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.0.3",
    "run-script-os": "^1.1.6"
  }
}
