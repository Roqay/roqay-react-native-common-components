{
  "name": "roqay-react-native-common-components-example",
  "description": "Example app for roqay-react-native-common-components",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "pods": "pod repo update && npx pod-install",
    "android": "react-native run-android",
    "ios-clean": "cd ios && rm -rf build && xcodebuild clean && cd ..",
    "ios-simulator": "react-native run-ios --simulator=\"iPhone 14 Pro Max\"",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "postinstall": "yarn pods && yarn android-clean && yarn ios-clean"
  },
  "dependencies": {
    "@klarna/react-native-vector-drawable": "^0.4.0",
    "@react-native-firebase/app": "^15.7.0",
    "@react-native-firebase/crashlytics": "^15.7.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "react": "18.1.0",
    "react-native": "0.70.2",
    "react-native-fast-image": "^8.6.1",
    "react-native-paper": "^4.12.5",
    "react-native-progress": "^5.0.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-svg": "^13.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-vector-image": "^0.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/runtime": "^7.19.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "metro-react-native-babel-preset": "^0.73.1",
    "reactotron-react-native": "^5.0.3"
  }
}
